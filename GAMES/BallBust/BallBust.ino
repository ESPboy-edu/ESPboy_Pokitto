        if( pok.update()) {
            PD::update();
            //pok.buttons.pollButtons();
            if (screen == 0) {
                pauseGame();
                if (pok.getTime() - freezeTimer > 200 && running) {
                    shakeScreen();
                    moveCorpse();
                    killCorpse();
                    enemyPlayerCollide();
                    killPlayer();
                    collidePlayerBall();
                    setSpd();
                    ballEnemyCollide();
                    frictionGravity();
                    frictionBall();
                    movePlayer();
                    moveEnemy();
                    moveBall();
                    containPlayer();
                    bounceBall();
                    containBall();
                    spawnEnemy();
                    riseEnemy();
                    killEnemy();
                }
                drawBackground();
                drawEnemy();
                drawCorpse();
                drawPlayer();
                drawArrow();
                drawBall();
                drawTpts();
                drawHeart();
                drawLife();
                drawScore();
                drawPause();
            } else if (screen == 1) {
                pok.display.drawBitmap(0, 0, gameover);
                if (pok.getTime() - eyesCounter > EYES_TIMER) {
                    drawEyes();
                }
                drawScore2();
                if (pok.buttons.bBtn()) {
                    resetGame();
                    pausePressed = true;
                }
            } else if (screen == 2) {
                if (pok.buttons.bBtn()) {
                    pausePressed = true;
                    screen = 0;
                }
                pok.display.drawBitmap(0, 0, start);
            }
        }
}
