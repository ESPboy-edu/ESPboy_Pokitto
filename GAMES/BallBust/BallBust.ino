float lenDirX(float len, int dir) {
    float pi = 3.14159265359;
    dir = ((-dir % 360)+ 360)% 360;
    float fDir = dir;
    return len * cos(pi * fDir / 180);
}

float lenDirY(float len, int dir) {
    float pi = 3.14159265359;
    dir = ((-dir % 360)+ 360)% 360;
    float fDir = dir;
    return len * sin(pi * fDir / 180);
}

int getAngle(long x1, long y1, long x2, long y2) {
    float i = y1 - y2;
    float j = x1 - x2;
    float pi = 3.14159265359;
    int angle = (atan2(i, j))*180 / pi;
    angle = (((-angle) % 360)+ 360)% 360;
    return angle;
}

