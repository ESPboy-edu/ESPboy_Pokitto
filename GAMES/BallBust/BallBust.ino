void setSpd() {
    if (pok.buttons.rightBtn() && hSpd < MAX_WALKSPD) {
        hSpd += acceleration + friction;
        lastDirRight = true;
    }
    if (pok.buttons.leftBtn() && hSpd > -MAX_WALKSPD) {
        hSpd -= acceleration + friction;
        lastDirRight = false;
    }
    if (currentPlayerY >= 64) {
        jumpTimer = pok.getTime();
        letGo = false;
    } else {
        if (!pok.buttons.aBtn()) {
            letGo = true;
        }
    }
    if (pok.buttons.aBtn() && pok.getTime() - jumpTimer < JUMP_TIME && !letGo) {
        vSpd = -jumpSpd;
    }
    return;
}

void frictionGravity() {
    vSpd += gravity;

    if (hSpd > 0) {
        if (hSpd - friction > 0) {
            hSpd -= friction;
        } else {
            hSpd = 0;
        }
    } else if (hSpd < 0) {
        if (hSpd + friction < 0) {
            hSpd += friction;
        } else {
            hSpd = 0;
        }
    }

    if (vSpd > 0) {
        if (vSpd - friction > 0) {
            vSpd -= friction;
        } else {
            vSpd = 0;
        }
    } else if (vSpd < 0) {
        if (vSpd + friction < 0) {
            vSpd += friction;
        } else {
            vSpd = 0;
        }
    }
    return;
}

