            ballDir = getAngle(ballCurrentX, ballCurrentY, currentPlayerX + 8, currentPlayerY + 12);
            ballSpd += KICK_FORCE;
            if (ballSpd > MAX_BALLSPD) {
                ballSpd = MAX_BALLSPD;
            }
        }
    } else {
        touchedBall = false;
    }
    return;
}

void frictionBall() {
    ballSpd -= ballFriction;
    if (ballSpd < 0) {
        ballSpd = 0;
    }
    return;
}

void bounceBall() {
    if (ballNextX >= 106 or ballNextX <= 4) {
        ballDir = (((180 - ballDir)% 360)+ 360)% 360;
    }
    if (ballNextY >= 83 or ballNextY <= 4) {
        ballDir = (((360 - ballDir)% 360)+ 360)% 360;
    }
    return;
}

