                enemyState[i] = false;

                corpseState = true;         //spawn corpse
                corpseX = enemyX[i];
                corpseY = enemyY[i];
                corpseDir = ballDir;
                corpseSpd = ballSpd/1.5;

                score += 10;
                tptsX = enemyX[i];
                tptsY = enemyY[i];
                tptsCounter = pok.getTime();
            }
        }
    }
    if (fatState) {
        if (fatHealth <= 0) {
            freezeTimer = pok.getTime();
            fatState = false;
            if (health < 5) {
                health += 1;
                heartCounter = pok.getTime();
                heartX = fatX;
                heartY = fatY;
            }
        }
    }
    return;
}

